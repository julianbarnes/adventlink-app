{
  "name": "adventlink-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "npm run build; node server.js",
    "start": "ng serve --configuration development",
    "build": "npm i --force; ng build",
    "build:dev": "npm i; ng build --configuration=development",
    "test": "ng test",
    "lint": "ng lint",
    "local": "npm update; ng serve --host 0.0.0.0 --disable-host-check",
    "e2e": "ng e2e",
    "heroku-postbuild": "ng build --configuration production ",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.5",
    "@angular/cdk": "^16.0.4",
    "@angular/cli": "~16.0.5",
    "@angular/common": "^16.0.5",
    "@angular/compiler": "^16.0.5",
    "@angular/compiler-cli": "^16.0.5",
    "@angular/core": "^16.0.5",
    "@angular/forms": "^16.0.5",
    "@angular/material": "^16.0.4",
    "@angular/platform-browser": "^16.0.5",
    "@angular/platform-browser-dynamic": "^16.0.5",
    "@angular/router": "^16.0.5",
    "bootstrap": "^5.2.3",
    "express": "^4.18.2",
    "hammerjs": "^2.0.8",
    "jquery": "^3.6.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.5",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.0.4"
  },
  "engines": {
    "node": "16.14.2",
    "npm": "8.5.0"
  }
}