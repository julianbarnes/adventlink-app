{
  "name": "adventlink-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "nvm use 14.0.0; npm run build; node server.js",
    "start:dev": "npm run build:dev; node server.js",
    "build": "npm i; ng build",
    "build:dev": "npm i; ng build --configuration=development",
    "test": "ng test",
    "lint": "ng lint",
    "local": "npm update; ng serve --host 0.0.0.0 --disable-host-check",
    "e2e": "ng e2e",
    "heroku-postbuild": "ng build --configuration production",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.0.1",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "bootstrap": "^5.2.3",
    "jquery": "^3.6.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.2",
    "@angular/cli": "~15.0.2",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.2"
  }, 
  "engines": {
    "node": "16.14.2",
    "npm": "8.5.0"
  }
}
